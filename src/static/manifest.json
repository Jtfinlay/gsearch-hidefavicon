{
    "name": "Hide Google Search Favicons",
    "version": "1.0.1",
    "description": "Hide favicons from your Google Search results! Less cluttered, less painful.",
    "content_scripts": [
        {
            "matches": ["*://*.google.com/search*"],
            "run_at": "document_end",
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
    },
    "manifest_version": 2
}
